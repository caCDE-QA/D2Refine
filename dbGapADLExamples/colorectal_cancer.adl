definition
	ITEM_GROUP[id1] matches {
 			item matches {
 				ELEMENT[id2] occurrences matches {1}
 				ELEMENT[id3] occurrences matches {1} matches {
 					value matches {
 						CODED_TEXT[id4]
 						CODED_TEXT[id5]
 					}	
 				}
 				ELEMENT[id6] occurrences matches {0..1} matches {
 					value matches {
 						INTERVAL_VALUE<Integer>[id7] occurrences matches {0..1} matches {
 							lower matches {33}
 							upper matches {99}
 						}
 						CODED_TEXT[id8]
 						CODED_TEXT[id9]
 					}
 				}
 				ELEMENT[id10] occurrences matches {1} matches {
 					value matches {
 						CODED_TEXT[id11]
 						CODED_TEXT[id12]
 						CODED_TEXT[id13]
 						CODED_TEXT[id14]
 					}
 				}
			}
 	}

